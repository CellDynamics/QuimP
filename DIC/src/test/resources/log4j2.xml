<?xml version="1.0" encoding="UTF-8"?><!--Piotr Baniukiewicz--><!--7.12.2015-->
<Configuration status="WARN">
	<Appenders>
		<Console target="SYSTEM_OUT" name="Console">
		<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"></PatternLayout></Console></Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="Console"></AppenderRef></Root></Loggers></Configuration>